<h2>Zaloguj się</h2>
<!-- <h2><%= Process.pid %></h2> this is to check process pids (eq if unicorns are changing-->
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <div><%= f.label "Adres email" %><br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label "Hasło" %><br />
  <%= f.password_field :password %></div>

  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label "Zapamiętaj mnie" %></div>
  <% end -%>

  <div><%= f.submit "Zaloguj" %></div>
<% end %>

<%= render "devise/shared/links" %>
